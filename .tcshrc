set     red="%{\033[1;31m%}"
set   green="%{\033[0;32m%}"
set  yellow="%{\033[1;33m%}"
set    blue="%{\033[1;34m%}"
set magenta="%{\033[1;35m%}"
set    cyan="%{\033[1;36m%}"
set   white="%{\033[0;37m%}"
set     end="%{\033[0m%}" # This is needed at the end... :(

set history = 10000
set savehist = ( 10000 merge )
set autoexpand
alias postcmd "history -S; history -M"


set GREP_COLORS = 33 
set filec
set autocorrect

umask 022
set autolist matchbeep=nomatch autoexpand autocorrect
set fignore=(.o ~)
# Ignore case while completing
set complete=enhance
unset rev rel pat
# Fancy prompt to the masses
if ($?prompt) then 
    set prompt="%{\033[1;36m%}%{\033[0;36m%}%{\033[1;32m%}%m %{\033[0;32m%}%{\033[1;35m%}%c8%{\033[33m%} >>%{\033[1;37m%} " # libi
endif
    
# Bindings 
bindkey '\e[1;3D' backward-word
bindkey '\e[1;3C' forward-word 
bindkey -k up history-search-backward
bindkey -k down history-search-forward
bindkey ^r i-search-back


alias gs git status
alias gc git checkout
alias ls ls -la

alias pycharm "nohup ~/Desktop/pycharm-2020.3.3/bin/pycharm.sh >& /dev/null &"
alias webstorm "nohup ~/Desktop/WebStorm-203.7148.54/bin/webstorm.sh >& /dev/null &"
